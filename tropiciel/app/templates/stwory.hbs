<div class="row">
  {{#each model as |stwor|}}
    <div class="col-xs-12 col-sm-3 text-center">
      <div class="media well">
        {{#link-to "stwor" stwor.id}}
          <img
            class="media-object thumbnail"
            src={{if stwor.zdjecie stwor.zdjecie "obrazy/pusty.png"}}
            alt={{stwor.nazwa}}
            width="100%"
            height="100%"
          >
        {{/link-to}}
        <div class="caption">
          <h3>{{stwor.nazwa}}</h3>
        </div>
      </div>
    </div>
  {{else}}
    <div class="jumbotron">
      <h1>Brak stwor√≥w</h1>
    </div>
  {{/each}} 
</div>